{
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "[c]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[django-html]": {
    "editor.defaultFormatter": "monosans.djlint",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.quickSuggestions": {
      "comments": true,
      "other": true,
      "strings": true
    },
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "monosans.djlint",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jinja-html]": {
    "editor.defaultFormatter": "monosans.djlint",
    "editor.detectIndentation": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.quickSuggestions": {
      "comments": true,
      "other": true,
      "strings": true
    }
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnPaste": false,
    "editor.formatOnSaveMode": "file",
    "editor.rulers": [92],
    "editor.tabSize": 4
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode",
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "django-html",
    "ejs",
    "jinja-html",
    "javascriptreact",
    "typescriptreact",
    "plaintext",
    "markdown",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)",
    "astro",
    "svelte"
  ],
  "auto-rename-tag.activationOnLanguage": [
    "jinja-html",
    "html",
    "django-html",
    "xml",
    "php",
    "javascript",
    "astro",
    "svelte"
  ],
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "cmake.configureOnOpen": true,
  "cSpell.userWords": ["htmx", "jsonable", "typeform", "Wernert"],
  "debug.console.fontSize": 13,
  "debug.toolBarLocation": "docked",
  "diffEditor.renderSideBySide": false,
  "djlint.formatCss": true,
  "djlint.formatJs": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActionsOnSave": [
    "source.organizeImports",
    "source.fixAll.eslint"
  ],
  "editor.fontFamily": "\"Dank Mono\", Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.hover.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true
  },
  "editor.tabSize": 4,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.type.class",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "django-html": "html",
    "javascript": "javascriptreact",
    "jinja-html": "html",
    "markdown": "html",
    "postcss": "css",
    "vue-html": "html"
  },
  "files.associations": {
    "*.css": "postcss",
    "*.jinja": "jinja-html",
    "*.js": "javascript",
    "*.scss": "scss",
    "*.ts": "typescript"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.mergeEditor": false,
  "githubPullRequests.pullBranch": "never",
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.scopes": ["document", "containers", "blocks"],
  "gitlens.views.formats.commits.label": "${❰ tips ❱❯  }${message}",
  "go.toolsManagement.autoUpdate": true,
  "html-css-class-completion.HTMLLanguages": [
    "html",
    "vue",
    "razor",
    "blade",
    "handlebars",
    "twig",
    "django-html",
    "php",
    "markdown",
    "erb",
    "ejs",
    "svelte",
    "jinja-html"
  ],
  "html.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "javascript.suggest.autoImports": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "license.author": "William Wernert (william.wernert@gmail.com)",
  "makefile.makefilePath": "Makefile",
  "markdownlint.config": {
    "MD033": {
      "allowed_elements": ["nobr"]
    }
  },
  // "mypy.runUsingActiveInterpreter": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingTypeStubs": "none"
  },
  "python.analysis.importFormat": "relative",
  // "python.analysis.inlayHints.functionReturnTypes": true,
  // "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.packageIndexDepths": [
    {
      "depth": 2,
      "name": "sklearn"
    },
    {
      "depth": 2,
      "name": "matplotlib"
    },
    {
      "depth": 2,
      "name": "scipy"
    },
    {
      "depth": 4,
      "name": "django"
    }
  ],
  "python.analysis.typeCheckingMode": "basic",
  "python.diagnostics.sourceMapsEnabled": true,
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.terminal.activateEnvironment": false,
  "redhat.telemetry.enabled": false,
  "scss.lint.unknownAtRules": "ignore",
  "settingsSync.ignoredSettings": ["-window.zoomLevel"],
  "stylelint.validate": ["css", "scss"],
  "svelte.enable-ts-plugin": true,
  "syntax.highlightLanguages": [
    "c",
    "cpp",
    "python",
    "go",
    "rust",
    "php",
    "ruby",
    "shellscript",
    "ocaml",
    "lua"
  ],
  "tailwindCSS.includeLanguages": {
    "jinja-html": "html",
    "astro": "html",
    "svelte": "html"
  },
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "\"Hack Nerd Font\", Monaco, 'Courier New', monospace",
  "todo-tree.highlights.useColourScheme": true,
  "typescript.autoClosingTags": true,
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "manual",
  "volar.autoCompleteRefs": true,
  "volar.format.initialIndent": {
    "html": true
  },
  "window.commandCenter": true,
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "statusBar.background": "#333333",
    "statusBar.debuggingBackground": "#263238",
    "statusBar.noFolderBackground": "#333333"
  },
  "workbench.editorAssociations": {
    "git-rebase-todo": "gitlens.rebase"
  },
  "workbench.iconTheme": "file-icons-colourless",
  "workbench.startupEditor": "none",
  "zenMode.hideTabs": false,
  "window.zoomLevel": 0.1
}
