Host *
    AddKeysToAgent yes
    UseKeychain yes

Host github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/keys/github_rwwiv

Host gitlab.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/keys/github_rwwiv

Host unraid.aperture.local
    User root
    PreferredAuthentications keyboard-interactive,password

Host soft
    HostName 192.168.9.10
    Port 23231
    AddKeysToAgent yes
    UseKeychain yes
