Include /Users/rwwiv/.colima/ssh_config

Host *
    AddKeysToAgent yes
    UseKeychain yes

Host github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/keys/github_rwwiv

Host gitlab.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/keys/github_rwwiv

Host unraid.aperture.local
    User root
    PreferredAuthentications keyboard-interactive,password
